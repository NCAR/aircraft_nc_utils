env.Install('$INSTALL_PREFIX/lib/nc2asc', './lib/header1_ames.txt')
env.Install('$INSTALL_PREFIX/lib/nc2asc', './lib/header2.txt')
env.Install('$INSTALL_PREFIX/lib/nc2asc', './lib/header2_ames.txt')
